{
  "name": "PFE_App",
  "version": "1.0.0",
  "private": true,
  "description": "App for PFE.",
  "scripts": {
    "start": "npm run dev",
    "build:front": "rm -rf dist && webpack --env.prod ",
    "watch:front": "webpack-dev-server --env.dev --open --watch --progress --colors ",
    "build:server": "tsc -p src/server && node build/server/app.js",
    "watch:server": "./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"npm run build:server\"",
    "clean": "rm -rf dist build",
    "dev": "concurrently --kill-others \"npm run watch:server\" \"npm run watch:front\"",
    "prod": "npm run build:front && npm run build:server",
    "test": "npm run test:jasmine || npm run test:e2e",
    "test:jasmine": "karma start ./config/karma.conf.js",
    "test:e2e": "ng e2e"
  },
